            adaptable="false"
            id="com.hundsun.ares.studio.procedure.ui.stock2.genprocedurecode"
            objectClass="com.hundsun.ares.studio.core.IARESResource">
         <action
               class="com.hundsun.ares.studio.procedure.ui.action.GenProcedureCodeAction"
               id="com.hundsun.ares.studio.cres.ui.action.genproccodewithnotpath"
               label="过程代码生成"
               menubarPath="com.hundsun.ares.studio.cres.ui.menu.genmodule/genmodule"
               state="true"
               style="push">
         </action>
         <visibility>
            <and>
               <objectClass
                     name="com.hundsun.ares.studio.core.IARESResource">
