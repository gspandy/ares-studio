   <extension
         point="org.eclipse.ui.popupMenus">
     <objectContribution
            adaptable="false"
            id="com.hundsun.ares.studio.procedure.ui.stock2.genpackagecode"
            objectClass="com.hundsun.ares.studio.core.IARESModule">
         <action
               class="com.hundsun.ares.studio.procedure.ui.action.GenPackageCodeAction"
               id="com.hundsun.ares.studio.cres.ui.action.genproccodewithnotpath"
               label="包头包体生成"
               menubarPath="com.hundsun.ares.studio.cres.ui.menu.genmodule/genmodule"
               state="true"
               style="push">
         </action>
         <visibility>
            <or>
               <objectState
                     name="rootType"
                     value="com.hundsun.ares.studio.procedure.resources.procedure">
               </objectState>
            </or>
         </visibility>
      </objectContribution>
      <objectContribution
            adaptable="false"
            id="com.hundsun.ares.studio.procedure.ui.stock2.genendcode"
            objectClass="com.hundsun.ares.studio.core.IARESModule">
         <action
               class="com.hundsun.ares.studio.procedure.ui.action.GenEndCodeAction"
               id="com.hundsun.ares.studio.cres.ui.action.genendwithchinesepath"
               label="后置代码生成"
               menubarPath="com.hundsun.ares.studio.cres.ui.menu.genmodule/genmodule"
               state="true"
               style="push">
