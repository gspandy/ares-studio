            order="100"
            pageid="com.hundsun.ares.studio.procedure.ui.extend.procedureextendpage">
      </pageItem>
      <pageItem
            class="com.hundsun.ares.studio.procedure.ui.extend.DatabaseExtendPageForStock2"
