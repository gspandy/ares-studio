<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<fragment>
   <extension
         point="com.hundsun.ares.studio.core.moduleRoot">
      <root
            icon="module_root.gif"
            id="test"
            name="测试模块根">
      </root>
   </extension>
   <extension
         point="com.hundsun.ares.studio.core.aresResource">
      <aresResource
            converter="com.hundsun.ares.studio.core.model.converter.PropertiesConvertor"
            icon="icons/action.gif"
            id="test"
            info-class="com.hundsun.ares.studio.core.ActionInfo"
            name="Test">
      </aresResource>
   </extension>
   <extension
         point="com.hundsun.ares.studio.core.rootToResTypeMaps">
      <map
            resTypes="test"
            rootType="test">
      </map>
   </extension>
   <extension
         point="com.hundsun.ares.studio.core.aresproject">
      <aresProject
            id="com.hundsun.ares.studio.core.test.aresProject1"
            name="name"
            natures="com.hundsun.ares.studio.core.test.aresProject1">
      </aresProject>
   </extension>
   <extension
         id="com.hundsun.ares.studio.core.testnature"
         name="NatureForTestUse"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="com.hundsun.ares.studio.internal.core.ARESProject">
         </run>
      </runtime>
      <requires-nature
            id="com.hundsun.ares.studio.aresnature">
      </requires-nature>
   </extension>
   <extension
         id="id1"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="com.hundsun.ares.studio.core.test.TestApplication">
         </run>
      </application>
   </extension>
      <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://www.hundsun.com/ares/studio/core/test"
            class="test.TestPackage"
            genModel="model/test.genmodel"/>
   </extension>

</fragment>
